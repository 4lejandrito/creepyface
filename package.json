{
  "name": "creepyface",
  "version": "1.0.0",
  "private": true,
  "main": "dist/creepyface.cjs.js",
  "module": "dist/creepyface.esm.js",
  "browser": "dist/creepyface.umd.js",
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "jest": "^20.0.4",
    "lolex": "^2.1.2",
    "rollup": "^0.47.2",
    "rollup-plugin-babel": "^3.0.1",
    "rollup-plugin-browsersync": "^0.2.2",
    "rollup-plugin-commonjs": "^8.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^2.0.1",
    "simulate-event": "^1.4.0"
  },
  "dependencies": {
    "data-attrs-to-js": "^1.1.1",
    "get-element-center": "^1.1.0",
    "image-promise": "^5.0.1",
    "lodash.throttle": "^4.1.1",
    "queryselectorall": "^0.1.0",
    "windrose": "^2.0.2",
    "zen-observable": "^0.5.2"
  },
  "scripts": {
    "start": "rollup -c -w",
    "build": "rollup -c",
    "build:netlify": "npm run build && cp -r test/* dist",
    "test": "jest"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "stage-0"
    ],
    "plugins": [
      "transform-class-properties"
    ],
    "env": {
      "test": {
        "presets": [
          "es2015",
          "stage-0"
        ]
      },
      "production": {
        "plugins": [
          "external-helpers"
        ]
      },
      "development": {
        "plugins": [
          "external-helpers"
        ]
      }
    }
  }
}
